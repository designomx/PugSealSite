	<script type="text/javascript">
		$(document).ready(function() {
			$('#fullpage').fullpage({
			anchors: ['firstPage', 'secondPage', '3rdPage', '4thpage', '5thPage', '6thpage', '7thpage', 'footer'],
			menu: '#menu',
			scrollBar: true,
			scrollingSpeed: 1000,
			afterLoad: function(anchorLink, index){
					var loadedSection = $(this);
					
					if(anchorLink == 'firstPage'){
						$('.top-brand-bar').addClass('initstate-bx');
						$('.close-ovrly').show();
					}
					
					if(anchorLink == '3rdPage'){
						$('.allanpoe-bx').addClass('slideInLeft back-o');
						$('.belisario-bx').addClass('slideInDown back-o');
						$('.tennyson-bx').addClass('slideInUp back-o');
					}
					
					if(anchorLink != '3rdPage'){
						$('.allanpoe-bx').removeClass('slideInLeft back-o');
						$('.belisario-bx').removeClass('slideInDown back-o');
						$('.tennyson-bx').removeClass('slideInUp back-o');
					}					
			
					if(anchorLink != 'firstPage'){
						$('.top-brand-bar').removeClass('initstate-bx');
						$('.close-ovrly').hide();
					}
					
				}
			});

		});
	</script>
	
